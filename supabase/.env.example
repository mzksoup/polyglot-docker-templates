# Supabase CLI 用プロジェクト名
SUPABASE_PROJECT_NAME=your-project-name

# Supabase local emulator 設定
SUPABASE_DB_PASSWORD=postgres
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: Supabase Studio ポートやDB設定（変更する場合のみ）
SUPABASE_STUDIO_PORT=3000
SUPABASE_DB_PORT=54322
SUPABASE_API_PORT=54321

# Custom API URL（必要な場合）
SUPABASE_API_URL=http://localhost:54321
SUPABASE_DB_URL=postgres://postgres:postgres@localhost:54322/postgres
